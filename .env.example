# VisionAssist Environment Configuration
# Copy this file to .env and modify as needed

# Server Configuration
HOST=127.0.0.1
PORT=5000
DEBUG=true
SECRET_KEY=dev-key-change-in-production

# CORS (use * for local development)
CORS_ORIGINS=*

# ML Backend
ML_BACKEND_ENABLED=true

# Logging
LOG_LEVEL=INFO

# Rate Limiting (optional for local dev)
RATE_LIMIT_ENABLED=false
